    <linearGradient id="rarity6">
      <stop offset="0%" stop-color={rawColors.accents.orangeLight}></stop>
      <stop offset="100%" stop-color={rawColors.accents.orange}></stop>
    </linearGradient>
    <linearGradient id="rarity5">
      <stop offset="0%" stop-color={rawColors.accents.yellowLight}></stop>
      <stop offset="100%" stop-color={rawColors.accents.yellow}></stop>
    </linearGradient>
    <linearGradient id="rarity4">
      <stop offset="0%" stop-color={rawColors.accents.purpleLight}></stop>
      <stop offset="100%" stop-color={rawColors.accents.purple}></stop>
    </linearGradient>
    <linearGradient id="rarity3">
      <stop offset="0%" stop-color={rawColors.accents.skyLight}></stop>
      <stop offset="100%" stop-color={rawColors.accents.sky}></stop>
    </linearGradient>
    <linearGradient id="rarity2">
      <stop offset="0%" stop-color={rawColors.accents.greenLight}></stop>
      <stop offset="100%" stop-color={rawColors.accents.green}></stop>
    </linearGradient>
  </defs>
</svg>
